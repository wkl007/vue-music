{
  "name": "vue-music-next",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:router": "vite build --config vite.router.js",
    "preview": "vite preview",
    "deploy": "npm run build && npm run build:router && pm2 start backend/prod.server.cjs -i max",
    "lint": "npm run lint:eslint && npm run lint:stylelint && npm run lint:prettier",
    "lint:eslint": "eslint --cache --fix \"**/*.{js,cjs,vue}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{css,less,vue}\"",
    "lint:prettier": "prettier --write \"**/*.{js,cjs,vue,css,less}\"",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "**/*.{js,cjs,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{css,less,vue}": [
      "stylelint --fix",
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@better-scroll/core": "^2.5.1",
    "@better-scroll/observe-dom": "^2.5.1",
    "@better-scroll/pull-up": "^2.5.1",
    "@better-scroll/slide": "^2.5.1",
    "@vueuse/core": "^12.7.0",
    "axios": "^1.8.1",
    "axios-retry": "^4.5.0",
    "core-js": "^3.40.0",
    "create-keyframe-animation": "^0.1.0",
    "good-storage": "^1.1.1",
    "lyric-parser": "^1.0.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vue3-lazy": "1.0.0-alpha.1",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@eslint/js": "^9.21.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "commitizen": "^4.3.1",
    "compression": "^1.8.0",
    "connect": "^3.7.0",
    "cookie-parser": "^1.4.7",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "js-base64": "^3.7.7",
    "less": "^4.2.2",
    "less-loader": "^12.2.0",
    "lint-staged": "^15.4.3",
    "pinyin": "2.11.2",
    "postcss-html": "^1.8.0",
    "postcss-less": "^6.0.0",
    "prettier": "^3.5.2",
    "qs": "^6.14.0",
    "serve-static": "^1.16.2",
    "stylelint": "^16.14.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended-vue": "^1.6.0",
    "stylelint-less": "^3.0.1",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.3",
    "vite": "^6.2.4",
    "vite-plugin-commonjs": "^0.10.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^0.21.1",
    "xsrf": "^1.0.0"
  }
}
