{
  "name": "vue-music",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "commit": "git-cz",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "npm run lint:code && npm run lint:style",
    "lint:code": "vue-cli-service lint",
    "lint:style": "vue-cli-service lint:style",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx,vue}": [
      "vue-cli-service lint"
    ],
    "src/**/*.{vue,htm,html,css,sss,less,scss}": [
      "vue-cli-service lint:style"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@better-scroll/core": "^2.2.1",
    "@better-scroll/observe-dom": "^2.2.1",
    "@better-scroll/pull-up": "^2.2.1",
    "@better-scroll/slide": "^2.2.1",
    "axios": "^0.21.1",
    "core-js": "^3.8.3",
    "create-keyframe-animation": "^0.1.0",
    "good-storage": "^1.1.1",
    "lyric-parser": "^1.0.1",
    "register-service-worker": "^1.7.2",
    "throttle-debounce": "^3.0.1",
    "vue": "^3.0.4",
    "vue-router": "^4.0.3",
    "vue3-lazy": "^1.0.0-alpha.1",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@samhammer/vue-cli-plugin-stylelint": "^2.0.1",
    "@types/good-storage": "^1.1.0",
    "@types/lyric-parser": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "@vue/cli-plugin-babel": "~5.0.0-alpha.8",
    "@vue/cli-plugin-eslint": "~5.0.0-alpha.8",
    "@vue/cli-plugin-pwa": "~5.0.0-alpha.8",
    "@vue/cli-plugin-router": "~5.0.0-alpha.8",
    "@vue/cli-plugin-typescript": "~5.0.0-alpha.8",
    "@vue/cli-plugin-vuex": "~5.0.0-alpha.8",
    "@vue/cli-service": "~5.0.0-alpha.8",
    "@vue/compiler-sfc": "^3.0.4",
    "@vue/eslint-config-standard": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "commitizen": "^4.2.3",
    "compression-webpack-plugin": "^7.1.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.20.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-vue": "^7.2.0",
    "husky": "^6.0.0",
    "less": "^4.0.0",
    "less-loader": "^8.0.0",
    "lint-staged": "^10.5.4",
    "stylelint": "^13.12.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-order": "^4.1.0",
    "terser-webpack-plugin": "^5.1.1",
    "typescript": "~4.2.3"
  }
}
