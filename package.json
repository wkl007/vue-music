{
  "name": "vue-music-next",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "npm run lint:eslint && npm run lint:stylelint && npm run lint:prettier",
    "lint:eslint": "eslint --cache --fix \"**/*.{js,cjs,vue}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{css,less,vue}\"",
    "lint:prettier": "prettier --write \"**/*.{js,cjs,vue,css,less}\"",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "**/*.{js,cjs,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{css,less,vue}": [
      "stylelint --fix",
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@better-scroll/core": "^2.5.1",
    "@better-scroll/observe-dom": "^2.5.1",
    "@better-scroll/pull-up": "^2.5.1",
    "@better-scroll/slide": "^2.5.1",
    "@vueuse/core": "^10.11.0",
    "axios": "^1.7.2",
    "axios-retry": "^4.4.0",
    "core-js": "^3.37.1",
    "create-keyframe-animation": "^0.1.0",
    "good-storage": "^1.1.1",
    "lyric-parser": "^1.0.1",
    "vue": "^3.4.21",
    "vue-router": "^4.3.3",
    "vue3-lazy": "1.0.0-alpha.1",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.4.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.4.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.26.0",
    "globals": "^15.4.0",
    "husky": "^9.0.11",
    "js-base64": "^3.7.7",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "lint-staged": "^15.2.7",
    "pinyin": "2.11.2",
    "postcss-html": "^1.7.0",
    "postcss-less": "^6.0.0",
    "prettier": "^3.3.2",
    "qs": "^6.12.1",
    "stylelint": "^16.6.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-less": "^3.0.1",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.0",
    "vite": "^5.2.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^0.20.0"
  }
}
